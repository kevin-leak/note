        "sidebar":{
            "Home":"https://www.crabglory.club/"
        }
    },
    "plugins":[
        "-lunr",
        "-search",
        "-livereload",
        "search-plus@^0.0.11",
        "github@^2.0.0",
        "github-buttons@2.1.0",
        "edit-link@^2.0.2",
        "advanced-emoji@^0.2.1",
        "anchors@^0.7.1",
        "splitter@^0.0.8",
        "tbfed-pagefooter@^0.0.1",
        "expandable-chapters-small@^0.1.7",
        "sectionx@^3.1.0",
        "local-video@^1.0.1",
        "favicon@^0.0.2",
        "page-toc-button" 
    ],
    "pluginsConfig":{
        "theme-default":{
            "showLevel":true
        },
        "github":{
            "url":"https://github.com/kevin-leak/note/tree/master/interview"
        },
        "github-buttons":{
            "repo":"kevin-leak/note/tree/master/interview",
            "types":[
                "star"
            ],
            "size":"small"
        },
        "sharing":{
            "weibo":true,
            "facebook":true,
            "twitter":true,
            "google":false,
            "instapaper":false,
            "vk":false,
            "all":[
                "facebook",
                "google",
                "twitter",
                "weibo",
                "instapaper"
            ]
        },
        "tbfed-pagefooter":{
            "copyright":"Copyright © ruheng.com 2017",
            "modify_label":"该文件修订时间：",
            "modify_format":"YYYY-MM-DD HH:mm:ss"
        },
        "edit-link":{
            "base":"https://github.com/kevin-leak/note/tree/master/interview",
            "label":"Edit This Page"
        },
        "sectionx":{
            "tag":"b"
        },
        "favicon":{
            "shortcut":"favicon.ico",
            "bookmark":"favicon.ico"
        },
        "page-toc-button": {
            "maxTocDepth": 8,
            "minTocSize": 2
        }
    }
}